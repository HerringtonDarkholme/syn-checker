{
  "j" : {
    "trailingSpace" : "grep -P ' +$'",
    "tabIndentation"     : "grep -P '^\\t'",
    "multipleAssignment" : "grep -P '^\\s*var\\s+.+?=.+?,.+?='",
    "leadingComma"   : "grep -P '^\\s*,'",
    "builtinLiteral" : "grep -P 'new (Array|Object)\\(\\)'"
  },

  "c" : {
    "variableNaming"     : "grep -P '\\$(?=[^ :,]*[^-a-z0-9 :,]+).+ *:'",
    "tabIndentation"     : "grep -P '^\\t'",
    "trailingSpace"      : "grep -P ' +$'",
    "commentStyle"       : "grep '\\*/'",
    "spaceDelimiter"     : "grep -P '[^ ]{|:[^ ].*?;|,[^ ]'",
    "multipleAttribute"  : "grep -P ';(?!base64|\\s*//).*?;($|})'",
    "zeroUnit"           : "grep -P ':([^0]*[ ,]0[^0 ;.,])+'",
    "deepNesting"        : "grep -P '^ {8,}'",
    "nestedIdSelector"   : "grep -P '^.{4,}#[^{].*\\{'",
    "pathHelper"         : "grep -P 'url(.*?)|image-url(.*?)'"
  },

  "h" : {
    "tabIndentation" : "grep -P '^\\t'",
    "traililngSpace" : "grep -P ' +$'",
    "doctype"        : "grep -P 'doctype [^5]'",
    "xssDoubleCurly" : "grep -P '#{{.*?}}'",
    "xssDoubleEq"    : "grep -P '^\\s*=='",
    "inlineTags"     : "grep -P '(?            :  : .*)\\w+\\s*=\\s*'",
    "defaultDiv"     : "grep -P '^div'",
    "redundantQuote" : "grep -P '=\"#{.*?}\"'"
  },

  "alias" : {
    "grep" : "/usr/local/bin/grep"
  }
}

